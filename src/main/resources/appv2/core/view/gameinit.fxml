<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="container" prefHeight="681.0" prefWidth="1013.0" stylesheets="@gameinit.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appv2.core.view.GameInitController">
   <children>
      <Label id="title" alignment="CENTER" stylesheets="@gameinit.css" text="Setup Game" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Button mnemonicParsing="false" onAction="#onCancelBtn" text="Cancel" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
      <Button id="primary-btn" layoutX="10.0" onAction="#onPlayBtn" layoutY="10.0" mnemonicParsing="false" stylesheets="@gameinit.css" text="Play" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Label id="error-msg" fx:id="errormessagefield" alignment="CENTER" stylesheets="@gameinit.css" text="SOME EXAMPLE ERROR MESSAGE" wrapText="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" />
      <VBox fx:id="playernamescontainer" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</AnchorPane>
